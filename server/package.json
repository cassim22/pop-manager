{
  "name": "pops-manager-server",
  "version": "1.0.0",
  "description": "Backend API para o sistema POPs Manager",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node --transpile-only src/index.ts",
    "dev:watch": "ts-node --transpile-only --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:init": "ts-node src/database/init.ts",
    "db:seed": "ts-node src/database/seed.ts"
  },
  "dependencies": {
    "pg": "^8.8.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^6.1.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^6.15.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/compression": "^1.7.2",
    "@types/morgan": "^1.9.4",
    "@types/pg": "^8.6.6",
    "@types/node": "^14.18.63",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "api",
    "express",
    "prisma",
    "postgresql",
    "pops-manager"
